/* pages/cup1/cup1.wxss */

page {
  background: #f6f6f6;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}

.content, .section_date, .section_level{
  font-size: 16px;
  text-align: center;
}

.name{
  font-size: 20px;
}

.unit, .today{
  padding-top: 50rpx;
}

.today, .yesterday, .thisweek, .lastweek{
  font-size: 14px;
}

.section_date{
  padding-top: 75rpx;
}

.section_level {
  margin-top: 40rpx;
  height: 140rpx;
  width: 80%;
  text-align: center;
  align-items: center;
}

.confirm{
  padding-top: 50rpx;
}